{
  "name": "rock-band",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "rock:lint": "nx workspace-lint && ng lint",
    "rock:format": "nx format:write",
    "rock:dev": "ng serve --project=rock-band --port 4100",
    "rock:storybook": "nx run rock-band:storybook",
    "rock:e2e": "ng e2e --project=rock-band-e2e",
    "rock:test": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "rock:prod": "ng build --prod --aot --project=rock-band",
    "rock:serve:prod": "ng serve --configuration=production",
    "rock:cart:test": "nx run cart:test --watch=false --code-coverage",
    "rock:product:test": "nx run product:test --watch=false --code-coverage",
    "rock:cc": "coverage/apps/rock-band/index.html -o",
    "rock:cart:cc": "coverage/libs/cart/index.html -o",
    "rock:product:cc": "coverage/libs/product/index.html -o"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@ngrx/effects": "^13.1.0",
    "@ngrx/entity": "^13.1.0",
    "@ngrx/store": "^13.1.0",
    "@ngrx/store-devtools": "^13.1.0",
    "@nrwl/angular": "13.10.1",
    "bootstrap": "^5.1.3",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@ng-bootstrap/ng-bootstrap": "^12.0.2",
    "@nrwl/cli": "13.10.1",
    "@nrwl/cypress": "13.10.1",
    "@nrwl/eslint-plugin-nx": "13.10.1",
    "@nrwl/jest": "13.10.1",
    "@nrwl/linter": "13.10.1",
    "@nrwl/storybook": "^13.10.1",
    "@nrwl/workspace": "13.10.1",
    "@storybook/addon-essentials": "~6.4.12",
    "@storybook/angular": "~6.4.12",
    "@storybook/builder-webpack5": "~6.4.12",
    "@storybook/core-server": "~6.4.12",
    "@storybook/manager-webpack5": "~6.4.12",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "cypress": "^9.1.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "nx": "13.10.1",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.6.2",
    "webpack": "^5.64.0"
  }
